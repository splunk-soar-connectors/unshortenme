    "configuration": {},
    "actions": [
        {
            "action": "test connectivity",
            "description": "Validate the asset configuration for connectivity. This action runs a quick query on the server to check the connection",
            "type": "test",
            "identifier": "test_asset_connectivity",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "lookup url",
            "description": "Get the original URL from a shortened URL",
            "type": "investigate",
            "identifier": "unshorten_url",
            "read_only": true,
            "parameters": {
                "url": {
                    "data_type": "string",
                    "description": "URL to unshorten",
                    "required": true,
                    "contains": [
                        "url",
                        "domain"
                    ],
                    "primary": true,
                    "order": 0
                }
            },
            "render": {
                "type": "table",
                "width": 4,
                "height": 5,
                "title": "Unshortened URLs"
            },
            "output": [
                {
                    "data_path": "action_result.parameter.url",
                    "data_type": "string",
                    "contains": [
                        "url",
                        "domain"
                    ],
                    "column_order": 0,
                    "column_name": "Short URL"
                },
                {
                    "data_path": "action_result.data.*.resolved_url",
                    "data_type": "string",
                    "column_order": 1,
                    "column_name": "Resolved URL"
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.data.*.success",
                    "data_type": "boolean"
                },
                {
                    "data_path": "action_result.data.*.usage_count",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.data.*.error",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.data.*.requested_url",
                    "data_type": "string",
                    "contains": [
                        "url"
                    ]
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric"
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric"
                },
                {
                    "data_path": "action_result.data.*.remaining_calls",
                    "data_type": "numeric",
                    "example_values": [
                        9
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                }
            ],
            "versions": "EQ(*)"
        }
    ],
    "pip39_dependencies": {
        "wheel": []
    },
    "pip313_dependencies": {
        "wheel": []
    }
}
